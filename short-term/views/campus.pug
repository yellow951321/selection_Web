extends mixins/layout

//- css
append head
  +shortCss('campus')

//- append buttons in the header
append buttons
  //- append buttons in the header
  include mixins/add-button

append content
  //- pop up deleteproject
  include mixins/sure-delete

  //- popup addproject
  include mixins/project-creation.pug

  include mixins/render-campus.pug
  //- content
  //- page management
  #page-management.ui.container.pusher
    if data.length
      each dataObj in data
        +renderCampus(dataObj)

append script
  +shortJS('campus')