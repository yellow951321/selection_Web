extends component/layout

//- css
append head
  +midLongCss('year')

//- append buttons in the header
include mixins/_addButton

append content
  //- pop up deleteproject
  include mixins/_sureDelete

  //- popup addproject
  include mixins/_projectCreation.pug

  include mixins/_renderYear.pug
  //- content
  //- page select
  #page-management.ui.container.pusher
    if yearFroms.length
      for yearFrom in yearFroms
        .year.ui.container
          h1.yearFrom.header= yearFrom.year
          for yearTo in yearFrom.info
            +renderYear(yearTo)


    else
      .ui.center.aligned.container
        div 請點擊右上角新增來新增專案

append script
  +midLongJS('year')