{
  "name": "selection-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node bin/server.bundle.js",
    "develop": "cross-env NODE_ENV=development npm-run-all --parallel watch nodemon browser-sync",
    "nodemon": "cross-env NODE_ENV=development nodemon --config nodemon.config.json bin/server.bundle.js",
    "browser-sync": "browser-sync --config browser-sync.config.js start",
    "lint": "npm-run-all --parallel lint:*",
    "lint:server-js": "eslint -c .eslintrc.server.js --ignore-path .gitignore --ignore-pattern \"!.eslintrc.*.js\" --fix \"server.js\" \".eslintrc.browser.js\" \".eslintrc.server.js\" \"browser-list.js\" \"browser-sync.config.js\" \"webpack.*.config.js\" \"auth/app.js\" \"auth/models/**/*.js\" \"short-term/app.js\" \"short-term/models/**/*.js\" \"mid-long-term/app.js\" \"mid-long-term/models/**/*.js\"",
    "lint:browser-js": "eslint -c .eslintrc.browser.js --ignore-path .gitignore --fix \"server.js\" \"auth/static/javascripts/**/*.js\" \"short-term/static/javascripts/**/*.js\" \"mid-long-term/static/javascripts/**/*.js\"",
    "pre-build": "cp config.js.default config.js",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel build:*",
    "build:server-js": "cross-env NODE_ENV=production webpack --config webpack.server.config.js",
    "build:browser-js": "cross-env NODE_ENV=production webpack --config webpack.browser.config.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:server-js": "webpack --watch --config webpack.server.config.js",
    "watch:browser-js": "webpack --watch --config webpack.browser.config.js",
    "b": "npm run build",
    "bs": "npm run build:server-js",
    "bb": "npm run build:browser-js",
    "d": "npm run develop",
    "l": "npm run lint",
    "ls": "npm run lint:server-js",
    "lb": "npm run lint:browser-js",
    "p": "npm run pre-build",
    "s": "npm run start",
    "w": "npm run watch",
    "ws": "npm run watch:server-js",
    "wb": "npm run watch:browser-js",
    "t": "webpack --watch --config webpack.transfer.config.js",
    "u": "webpack --watch --config webpack.upload.config.js",
    "c": "webpack --watch --config webpack.check.config.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.26.5",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "csv-parser": "^2.2.0",
    "csv-writer": "^1.2.0",
    "d3": "^5.7.0",
    "dotenv": "^7.0.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "jsdom": "^13.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.3",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.4",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "pug": "2.0.0-beta11",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "sequelize": "^4.43.1",
    "style-loader": "^0.23.1",
    "unique-filename": "^1.1.1",
    "webpack": "^4.29.6",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "eslint": "^5.12.0",
    "eslint-config-google": "^0.11.0",
    "nodemon": "^1.18.9",
    "webpack-cli": "^3.3.0"
  }
}
