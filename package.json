{
  "name": "selection-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env MODE=PRODUCTION node app.js",
    "s": "npm run start",
    "develop": "cross-env MODE=DEVELOPMENT nodemon app.js",
    "d": "npm run develop",
    "lint:backend": "eslint -c .eslintrc.backend.js --ignore-path .gitignore --ignore-path .eslintignore.backend --fix \"**/*.js\"",
    "lint:frontend": "eslint -c .eslintrc.frontend.js --ignore-path .gitignore --fix \"public/**/*.js\"",
    "lb": "npm run lint:backend",
    "lf": "npm run lint:frontend",
    "l": "npm run lb && npm run lf",
    "preset": "cp db/config.js.default db/config.js",
    "p": "npm run preset"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "mongoose": "^5.4.3",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "uid-safe": "^2.1.5"
  },
  "devDependencies": {
    "eslint": "^5.12.0",
    "eslint-config-google": "^0.11.0",
    "nodemon": "^1.18.9"
  }
}
