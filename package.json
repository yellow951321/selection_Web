{
  "name": "selection-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node bin/server.bundle.js",
    "s": "npm run start",
    "develop": "cross-env NODE_ENV=development nodemon bin/server.bundle.js",
    "d": "npm run develop",
    "lint:backend": "eslint -c .eslintrc.backend.js --ignore-path .gitignore --ignore-path .eslintignore.backend --fix \"**/*.js\"",
    "lint:frontend": "eslint -c .eslintrc.frontend.js --ignore-path .gitignore --fix \"public/**/*.js\"",
    "lb": "npm run lint:backend",
    "lf": "npm run lint:frontend",
    "l": "npm run lb && npm run lf",
    "preset": "cp config.js.default config.js",
    "p": "npm run preset",
    "w": "webpack --config webpack.config.js && npm run d",
    "ww": "webpack --config webpack.config.js --watch",
    "t": "webpack --config webpack.transfer.config.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "~1.4.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "csv-parser": "^2.2.0",
    "csv-writer": "^1.2.0",
    "d3": "^5.7.0",
    "dotenv": "^7.0.0",
    "express": "~4.16.0",
    "express-session": "^1.15.6",
    "jsdom": "^13.2.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.4.3",
    "morgan": "~1.9.0",
    "mysql2": "^1.6.4",
    "pug": "2.0.0-beta11",
    "pug-loader": "^2.4.0",
    "sequelize": "^4.43.1",
    "style-loader": "^0.23.1",
    "unique-filename": "^1.1.1",
    "webpack": "^4.29.6",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "eslint": "^5.12.0",
    "eslint-config-google": "^0.11.0",
    "nodemon": "^1.18.9",
    "webpack-cli": "^3.3.0"
  }
}
